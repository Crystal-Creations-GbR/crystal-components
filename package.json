    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-storybook": "^0.6.14",
    "eslint-plugin-vue": "^9.21.1",
    "prettier": "^3.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.68.0",
    "semantic-release": "^22.0.5",
    "storybook": "7.6.12",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
